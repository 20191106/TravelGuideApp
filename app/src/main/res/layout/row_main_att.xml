<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/box_back_icon01">
    <TextView
        android:id="@+id/main_att_filterTv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="동남아"
        android:textSize="25sp"
        android:gravity="center"
        android:padding="10dp"/>
    <TextView
        android:id="@+id/main_att_placeTv"
        android:layout_toRightOf="@id/main_att_filterTv"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="호치민"
        android:textSize="25sp"
        android:gravity="center"
        android:padding="10dp"/>
    <LinearLayout
        android:id="@+id/main_att_stars"
        android:layout_below="@+id/main_att_filterTv"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/main_att_star_1"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/top_bookmark_on"/>
        <ImageView
            android:id="@+id/main_att_star_2"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/top_bookmark_on"/>
        <ImageView
            android:id="@+id/main_att_star_3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/top_bookmark_on"/>
        <ImageView
            android:id="@+id/main_att_star_4"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/top_bookmark_on"/>
        <ImageView
            android:id="@+id/main_att_star_5"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/top_bookmark_on"/>
    </LinearLayout>
    <ImageView
        android:id="@+id/main_att_mark"
        android:layout_alignParentRight="true"
        android:layout_width="50dp"
        android:layout_height="50dp"/>
    <LinearLayout
        android:id="@+id/main_att_images"
        android:layout_below="@+id/main_att_stars"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:orientation="horizontal">
    </LinearLayout>
</RelativeLayout>